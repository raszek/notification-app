{% extends 'base.html.twig' %}


{% block body %}
<div class="mt-3">
    {{ form_start(form) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.email) }}
    {{ form_row(form.phone) }}

    <input type="submit" value="Create" class="btn btn-primary">
    <a href="{{ path('app_person_list') }}" class="btn btn-secondary ms-2">Back</a>
    {{ form_end(form) }}
</div>
{% endblock %}
